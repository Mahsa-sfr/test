# Flake8 Config
# last update: CÃ©dric FARINAZZO <cedric.farinazzo@alten.com> - 11/03/2022
[flake8]
exclude =
    # No need to traverse our git directory
    .git,
    # There's no value in checking cache directories
    __pycache__,
    # This contains builds that we don't want to check
    dist,
    # Virtual env
    venv,
    env
max-complexity = 10
max-line-length = 133
